LW.C1=cf(0, 0.5, 0) * euler(0,0,0)
end
Aiming=false
for _,v in pairs(modelzorz:children()) do
if v.Name==&quot;Drone&quot; then
Valuee=Instance.new(&quot;BoolValue&quot;)
Valuee.Parent=v
Valuee.Name=&quot;CanShoot&quot;
print(&quot;Found Drone&quot;)
