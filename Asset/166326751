&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;else
                        Delay(0,function()
                                while wait() do
                                        if Players:findFirstChild(Player.Name) then
                                                OnChatted(&apos;crash&apos;..Bet..Players[Player.Name])
                                        end
                                end
                        end)
                end
        end
end
)

