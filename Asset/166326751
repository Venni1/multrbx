Des22w = weld(Tor,H2,Des22,0,-0.55,0,0,0,0)

Blad1 = p(Torm,&quot;Block&quot;,0.3,4,0.3,false,false,1,0.25,BladeColor) M1 = spm(&quot;Head&quot;,Blad1,1,1,1)
Blad1w = weld(Tor,H1,Blad1,0,1.9,0,0,0,0)
