&#9;&#9;neck.C0 = necko * CA(MR(-5-10*i), 0, MR(-35-25*i))

&#9;&#9;wait()

&#9;end

&#9;HW.Part1 = nil

&#9;TH.Part1 = Handle

&#9;for i = 0.09, 1, 0.18 do

&#9;&#9;RAW.C0 = CA(MR(220-220*i), MR(-30+30*i), MR(20-55+35*i))

&#9;&#9;neck.C0 = necko * CA(MR(-5-10+15*i), 0, MR(-35-25+60*i))

&#9;&#9;wait()

&#9;end

&#9;neck.C0 = necko

&#9;Detach()

end



function setatk(s, e)

&#9;Notime(function()

&#9;&#9;Attack = e

&#9;&#9;wait(0.25)

&#9;&#9;Attack = s

&#9;end)

end



