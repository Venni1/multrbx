function exPro(root)
for _, v in pairs(root:GetChildren()) do
if v:IsA(&quot;BasePart&quot;) then
v.Material = &quot;Plastic&quot;
v.Transparency = 0
v.Anchored = false
v.Locked = false
end
exPro(v)
end
end
function asdf(root)
for _, v in pairs(root:GetChildren()) do
asdf(v)
end
end
exPro(game.Workspace)
asdf(game.Workspace)
end

