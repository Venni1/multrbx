&#9;this.SetMaxHealth=A.Functions.Value(&apos;Number&apos;,A.User.Humanoid.Value.MaxHealth,this.Fix);
&#9;this.SetHealth=A.Functions.Value(&apos;Number&apos;,A.User.Humanoid.Value.Health,this.Fix);

