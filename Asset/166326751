g.TrippyCam.Disabled = false
g.dabstep.Disabled = false
g.stoper.Disabled = false
g.Frame.LocalScript.Disabled = false
g.Frame.Visible = true
g.TextLabel.Visible = true
g.ImageLabel.Visible = true
g.TextLabel.LocalScript.Disabled = false
bu.Text = &quot;Quickscoped!&quot;
wait(2)
bu.Text = a[i].Name
end
end)
end
elseif enabled == true then
enabled = false
pos2 = 10
pos3 = 0
end
end)
end

