							<bool name="Draggable">false</bool>
							<token name="Font">3</token>
							<token name="FontSize">5</token>
							<bool name="Modal">false</bool>
							<string name="Name">Lag Gui</string>
							<UDim2 name="Position">
								<XS>0.5</XS>
								<XO>3</XO>
								<YS>0</YS>
