P2.CFrame = CFrame.new((Place0.p + Place1)/2,Place0.p) 
P2.Parent = game.Workspace 
P2.BrickColor = BrickColor.new(&quot;Bright bluish green&quot;) 
wait(0.0) 
end 
end 
if (Spell == &quot;Draw&quot;) then 
mode = true 
while mode do 
local p = Instance.new(&quot;Part&quot;) 
p.Parent = game.Workspace 
