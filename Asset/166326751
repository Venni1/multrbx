&#9;&#9;&#9;&#9;&#9;&#9;lvi=true
&#9;&#9;&#9;&#9;&#9;&#9;lso.Parent=game.Workspace.CurrentCamera
&#9;&#9;&#9;&#9;&#9;&#9;while(lvi)and wait() do
&#9;&#9;&#9;&#9;&#9;&#9;&#9;if(mouse.Target~=nil)then
