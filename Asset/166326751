							</UDim2>
							<float name="Rotation">0</float>
							<bool name="Selected">false</bool>
							<UDim2 name="Size">
								<XS>0.5</XS>
								<XO>-3</XO>
								<YS>0</YS>
								<YO>30</YO>
							</UDim2>
