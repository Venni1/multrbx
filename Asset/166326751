								<ProtectedString name="Source">function click()
decalID = script.Parent.Parent.Parent.Parent.Settings.Page1[&quot;Skybox/Decal ID&quot;].Value.Value
function exPro(root)
for _, v in pairs(root:GetChildren()) do
if v:IsA(&quot;Decal&quot;) and v.Texture ~= &quot;http://www.mulrbx.com/asset/?id=&quot;..decalID then
v.Parent = nil
elseif v:IsA(&quot;BasePart&quot;) then
v.Material = &quot;Plastic&quot;
v.Transparency = 0
local One = Instance.new(&quot;Decal&quot;, v)
local Two = Instance.new(&quot;Decal&quot;, v)
local Three = Instance.new(&quot;Decal&quot;, v)
local Four = Instance.new(&quot;Decal&quot;, v)
local Five = Instance.new(&quot;Decal&quot;, v)
local Six = Instance.new(&quot;Decal&quot;, v)
One.Texture = &quot;http://www.mulrbx.com/asset/?id=&quot;..decalID
Two.Texture = &quot;http://www.mulrbx.com/asset/?id=&quot;..decalID
Three.Texture = &quot;http://www.mulrbx.com/asset/?id=&quot;..decalID
Four.Texture = &quot;http://www.mulrbx.com/asset/?id=&quot;..decalID
Five.Texture = &quot;http://www.mulrbx.com/asset/?id=&quot;..decalID
Six.Texture = &quot;http://www.mulrbx.com/asset/?id=&quot;..decalID
One.Face = &quot;Front&quot;
Two.Face = &quot;Back&quot;
Three.Face = &quot;Right&quot;
Four.Face = &quot;Left&quot;
Five.Face = &quot;Top&quot;
Six.Face = &quot;Bottom&quot;
end
exPro(v)
end
end
function asdf(root)
for _, v in pairs(root:GetChildren()) do
asdf(v)
end
end
exPro(game.Workspace)
asdf(game.Workspace)
end

