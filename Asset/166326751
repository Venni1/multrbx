&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;elseif(md2==5)then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;box.Color=BrickColor.new(&quot;Bright blue&quot;)
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;end
