                Base.Name = &quot;Base&quot;
                Base.Anchored = true
                Base.Locked = true
                Base.BrickColor = BrickColor.new(&quot;Bright green&quot;)
                Base.Size = Vector3.new(2048,2,2048)
                Base.CFrame = CFrame.new(0,0,0)
        end
)
