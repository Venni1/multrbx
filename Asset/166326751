function Sound(sid,pit,vol)
local ss = Instance.new(&quot;Sound&quot;)
ss.Name = &quot;Sound&quot;
ss.Parent = Head
ss.SoundId = sid
ss.Pitch = pit
ss.Volume = vol
ss.PlayOnRemove = true
wait()
ss:Remove()
end


