Glow1w = weld(Tor,H1,Glow1,0,1.9,0,0,0,0)
Glow2 = p(Torm,&quot;Block&quot;,0.47,4.2,0.47,false,false,1,0,BladeColor) GM2 = spm(&quot;Head&quot;,Glow2,1,1,1)
Glow2w = weld(Tor,H2,Glow2,0,1.9,0,0,0,0)

r(Bp,HopperName)
bin = inew(&quot;HopperBin&quot;)
bin.Name = HopperName
bin.Parent = Bp

Gui = Instance.new(&quot;ScreenGui&quot;,Pg)
