bleedm.MeshType = &quot;Sphere&quot; 
bleedm.Scale = Vector3.new(.2,.2,.2) 
bleedm.Parent = bleed 
game:GetService(&quot;Debris&quot;):AddItem(bleed, 5) 
end 
end 
end 
