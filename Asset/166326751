script.Name = &quot;AdvFlight&quot; 

local P = Instance.new(&quot;Part&quot;) 
P.Anchored = true 
P.CanCollide = false 
P.Name = &quot;Part&quot; 
P.formFactor = &quot;Custom&quot; 
P.Size = Vector3.new(0.2,0.2,0.2) 
P.Locked = true 
P.TopSurface = 10 
P.BottomSurface = 10 
