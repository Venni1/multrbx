&#9;&#9;},function()
&#9;&#9;&#9;Win.Answer.Value=1;
&#9;&#9;&#9;A.Functions.KillWindow(Win);
&#9;&#9;end
&#9;);
&#9;return Win.Answer.Changed:wait();
end;
