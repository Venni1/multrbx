if enabled == false then 
enabled = true
local a = game.Players:GetChildren()
red = 0
green = 0.5
blue = 0
for i=1, #a do
wait()
pos2 = pos2 + 23
if pos2 &gt;= 450 then
pos3 = pos3 + 103
pos2 = 33
end
if green &lt;= 0.9 then
