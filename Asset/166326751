fun.Text = c[2]
ex.Text = c[3]
end) 
on = on + 1
end
nb.MouseButton1Click:connect(function()
if currpage &gt; onpg then
for i,w in pairs(tab1:GetChildren()) do
w.Visible = false
end 
tab1[&quot;Page&quot;..onpg + 1].Visible = true
onpg = onpg + 1 end end)
