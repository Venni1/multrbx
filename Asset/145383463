&#9;&#9;Base = &quot;Unknown line: Unknown error(Suggestion: Probobly not in your level/identity)&quot;
&#9;else
&#9;&#9;Base = ErrorVar:match&apos;&quot;*&quot;.:(.+)&apos; or &quot;Unknown line: &quot;..ErrorVar
&#9;end
&#9;MakeNode(&quot;Error&quot;, &quot;ERROR: &quot;..Base, Player)
end

