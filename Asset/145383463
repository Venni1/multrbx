&#9;&#9;&#9;&#9;if User then
&#9;&#9;&#9;&#9;&#9;SendMessage(User, Config.Name, Obj.Value, 5)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;Config:Destroy()
&#9;end
end)

if Bet == &quot;/&quot; or Bet == &quot;-&quot; then Bet = &quot; &quot; end
if VIPMemberID == 0 then VIPMemberID = 1 end
if VIPAdminID == 0 then VIPAdminID = 1 end

