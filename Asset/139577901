	if not Player or not Client or Client ~= Player or not Key or not Tool.Enabled then return end
	if Key == Enum.KeyCode.Q then
		if IsPeriSparkling() then
			OrnamentAttack()
		end
	elseif Key == Enum.KeyCode.E then
		
	end	
