local Properties = {
	BaseDamage = 27,
	SpecialCooldown = 60,
	OrnamentDuration = 20,
	OrnamentScaleRadius = 18.36,
	BaseUrl = "rbxassetid://"
}

local Remote = (Tool:FindFirstChild("Remote") or Instance.new("RemoteEvent"));Remote.Name = "Remote";Remote.Parent = Tool

