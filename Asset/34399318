		end

		cScript = script:FindFirstChild("CakeScript")
		if (cScript == nil) then print("CSCRIPT NOT CLONEABLE!") end
		newCScript = cScript:Clone()
		newCScript.Parent = myPlayer.PlayerGui
		newCScript.Disabled = false
	end

	ball.Parent = nil

--	HitSound:Play()
