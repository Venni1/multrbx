local HitSound = Instance.new("Sound")
HitSound.SoundId = "http://www.mulrbx.com/asset/?id=19321040"
HitSound.Parent = arrow
HitSound.Volume = 1

function onTouched(hit)

	connection:disconnect()

	if hit.Name == "XMAS" then
		HitSound:Play()
