&#9;sound.Parent = bomb
&#9;sound:Play()

&#9;explosion:Stop()

&#9;local mesh = Instance.new(&quot;SpecialMesh&quot;)
&#9;mesh.MeshId = &quot;http://www.mulrbx.com/asset/?id=28511843&quot;
&#9;mesh.TextureId = &quot;http://www.mulrbx.com/asset/?id=28512114&quot;
&#9;mesh.Parent = bomb

&#9;local creator_tag = Instance.new(&quot;ObjectValue&quot;)
&#9;creator_tag.Value = vPlayer
&#9;creator_tag.Name = &quot;creator&quot;
&#9;creator_tag.Parent = bomb

