function updateHats()
&#9;for _,v in pairs(script.Parent:GetChildren()) do
&#9;&#9;if v.Name == &quot;CombineHat&quot; then
&#9;&#9;&#9;v:Destroy()
&#9;&#9;end
&#9;end
&#9;if player.PurchasedHats.CurrentHat.Value ~= 0 then
&#9;&#9;local hat = game:GetService(&quot;InsertService&quot;):LoadAsset(hats[player.PurchasedHats.CurrentHat.Value]):WaitForChild(&quot;CombineHat&quot;)
&#9;&#9;hat.Parent = script.Parent
&#9;end
&#9;if player.PurchasedHats.CurrentHat2.Value ~= 0 then
&#9;&#9;local hat = game:GetService(&quot;InsertService&quot;):LoadAsset(hats[player.PurchasedHats.CurrentHat2.Value]):WaitForChild(&quot;CombineHat&quot;)
&#9;&#9;hat.Parent = script.Parent
&#9;end&#9;
end

updateHats()

