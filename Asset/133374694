function onDied()
&#9;local id = nil
&#9;local v = Instance.new(&quot;IntValue&quot;)
&#9;v.Value = 1
&#9;v.Name = &quot;Upgrade&quot;
&#9;local player = game.Players:GetPlayerFromCharacter(script.Parent)
&#9;if player ~= nil then
&#9;&#9;v.Value = player.PurchasedUpgrades.CurrentUpgrade.Value
&#9;else
&#9;&#9;print(&quot;Can&apos;t find player&quot;)
&#9;end
&#9;if script.Parent.MakeStalker.Value == true then
&#9;&#9;id = 130951292
&#9;else
&#9;&#9;if player.PurchasedUpgrades.CurrentUpgrade.Value == 7 then
&#9;&#9;&#9;id = 126014152
&#9;&#9;else
&#9;&#9;&#9;id = 130948290
&#9;&#9;end
&#9;end
&#9;local pos = script.Parent.Torso.Position
&#9;local me = script.Parent
&#9;if isInLobby() then return end
&#9;script.Parent = game.Workspace
&#9;local stuff = me:GetChildren()
&#9;local hats = {}
&#9;local ragdoll = game:GetService(&quot;InsertService&quot;):LoadAsset(id).Ragdoll
&#9;ragdoll.Parent = game.Workspace
&#9;ragdoll:MoveTo(pos)
&#9;v.Parent = ragdoll
&#9;if id == 130948290 then
&#9;&#9;for i,v in pairs(ragdoll:GetChildren()) do
&#9;&#9;&#9;if v:IsA(&quot;Shirt&quot;) then
&#9;&#9;&#9;&#9;v.ShirtTemplate = me.Shirt.ShirtTemplate
&#9;&#9;&#9;elseif v:IsA(&quot;Pants&quot;) then
&#9;&#9;&#9;&#9;v.PantsTemplate = me.Pants.PantsTemplate
&#9;&#9;&#9;elseif v:IsA(&quot;CharacterMesh&quot;) then
&#9;&#9;&#9;&#9;v.BaseTextureId = me.SpaceTrooperTorso.BaseTextureId
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;ragdoll.Hat.Handle.Mesh.TextureId = me.Head.Mesh.TextureId
&#9;end
&#9;for i = 1,#stuff do
&#9;&#9;if stuff[i].Name == &quot;CombineHat&quot; then
&#9;&#9;&#9;table.insert(hats,stuff[i])
&#9;&#9;&#9;stuff[i].Parent = game.Lighting
&#9;&#9;else
&#9;&#9;&#9;stuff[i]:Destroy()
&#9;&#9;end
&#9;end
&#9;if #hats &gt; 0 then
&#9;&#9;for _,v in pairs(hats) do
&#9;&#9;&#9;v.Parent = ragdoll
&#9;&#9;end
&#9;end
&#9;script:Destroy()
end

