local prev 
local parts = script.Parent:GetChildren() 

for i = 1,#parts do 
&#9;if parts[i]:IsA(&quot;BasePart&quot;) then
&#9;&#9;if (prev ~= nil)then 
&#9;&#9;&#9;local weld = Instance.new(&quot;Weld&quot;) 
&#9;&#9;&#9;weld.Part0 = prev 
&#9;&#9;&#9;weld.Part1 = parts[i] 
&#9;&#9;&#9;weld.C0 = prev.CFrame:inverse() 
&#9;&#9;&#9;weld.C1 = parts[i].CFrame:inverse() 
&#9;&#9;&#9;weld.Parent = prev 
&#9;&#9;end 
&#9;&#9;prev = parts[i] 
&#9;end 
end

--Credits to NyroKoSu and OrientalAura</ProtectedString>
