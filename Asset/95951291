				<ProtectedString name="Source"><![CDATA[script.Parent.Equipped:connect(function(mouse)
	mouse.KeyDown:connect(function(key)
		script.Parent.KeyDown:FireServer(key)
	end)
end)]]></ProtectedString>
			</Properties>
		</Item>
